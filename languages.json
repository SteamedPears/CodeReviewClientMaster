{
	"include_path":"include/CodeMirror/mode/",
	"data":{
		"none":{"mode":"text/plain",
				"requires":[]},
		"asp":{"mode":"application/x-aspx",
			   "requires":["htmlmixed","clike","htmlembedded"]},
		"clike":{"file":"clike/clike.js"},
		"c":{"mode":"text/x-csrc",
			 "options":{"useCPP":true},
			 "requires":["clike"]},
		"cpp":{"mode":"text/x-c++src",
			   "options":{"useCPP":true},
			   "requires":["clike"]},
		"csharp":{"mode":"text/x-csharp",
				  "requires":["clike"]},
		"clojure":{"file":"clojure/clojure.js",
				   "mode":"text/x-clojure"},
		"coffeescript":{"file":"coffeescript/coffeescript.js",
						"mode":"text/x-coffeescript"},
		"css":{"file":"css/css.js",
			   "mode":"text/css"},
		"diff":{"file":"diff/diff.js",
				"mode":"text/x-diff"},
		"ecl":{"file":"ecl/ecl.js",
			   "mode":"text/x-ecl"},
		"ejs":{"mode":"application/x-ejs",
			   "requires":["htmlmixed","htmlembedded"]},
		"erlang":{"file":"erlang/erlang.js",
				  "mode":"text/x-erlang"},
		"gfm":{"file":"gfm/gfm.js",
			   "mode":"gfm",
			   "requires":["markdown",
						   "asp",
						   "c",
						   "cpp",
						   "csharp",
						   "clojure",
						   "coffeescript",
						   "diff",
						   "ecl",
						   "erlang",
						   "go",
						   "groovy",
						   "haskell",
						   "haxe",
						   "html",
						   "java",
						   "javascript",
						   "jinja2",
						   "json",
						   "jsp",
						   "stex",
						   "less",
						   "lua",
						   "markdown",
						   "mysql",
						   "ntriples",
						   "ocaml",
						   "pascal",
						   "perl",
						   "php",
						   "pig",
						   "plsql",
						   "properties",
						   "python",
						   "r",
						   "rpm-spec",
						   "rpm-changes",
						   "rst",
						   "ruby",
						   "rust",
						   "scala",
						   "scheme",
						   "shell",
						   "smalltalk",
						   "smarty",
						   "sparql",
						   "stex",
						   "tiddlywiki",
						   "tikiwiki",
						   "vbscript",
						   "velocity",
						   "verilog",
						   "xml",
						   "xquery",
						   "yaml"]},
		"go":{"file":"go/go.js",
			  "mode":"text/x-go"},
		"groovy":{"file":"groovy/groovy.js",
				  "mode":"text/x-groovy"},
		"haskell":{"file":"haskell/haskell.js",
				   "mode":"text/x-haskell"},
		"haxe":{"file":"haxe/haxe.js",
				"mode":"text/x-haxe"},
		"html":{"mode":"text/html",
				"requires":["xml"],
				"options":{"htmlMode":true}},
		"htmlmixed":{"file":"htmlmixed/htmlmixed.js",
					 "mode":"text/html",
					 "requires":["xml","javascript","css"]},
		"htmlembedded":{"file":"htmlembedded/htmlembedded.js"},
		"java":{"mode":"text/x-java",
				"requires":["clike"]},
		"javascript":{"file":"javascript/javascript.js",
					  "mode":"text/javascript"},
		"jinja2":{"file":"jinja2/jinja2.js",
				  "mode":"jinja2"},
		"json":{"mode":"application/json",
				"requires":["javascript"],
				"options":{"json":true}},
		"jsp":{"mode":"application/x-jsp",
			   "requires":["htmlmixed","clike","htmlembedded"]},
		"latex":{"requires":["stex"]},
		"less":{"file":"less/less.js",
				"mode":"text/x-less"},
		"lua":{"file":"lua/lua.js",
			   "mode":"text/x-lua"},
		"markdown":{"file":"markdown/markdown.js",
					"mode":"text/x-markdown",
					"requires":["xml"]},
		"mysql":{"file":"mysql/mysql.js",
				 "mode":"text/x-mysql"},
		"ntriples":{"file":"ntriples/ntriples.js",
					"mode":"text/n-triples"},
		"ocaml":{"file":"ocaml/ocaml.js",
				 "mode":"text/x-ocaml"},
		"pascal":{"file":"pascal/pascal.js",
				  "mode":"text/x-pascal"},
		"perl":{"file":"perl/perl.js",
				"mode":"text/x-perl"},
		"php":{"file":"php/php.js",
			   "mode":"application/x-httpd-php",
			   "requires":["xml","javascript","css","clike"]},
		"pig":{"file":"pig/pig.js",
			   "mode":"text/x-pig"},
		"plsql":{"file":"plsql/plsql.js",
				 "mode":"text/x-plsql"},
		"properties":{"file":"properties/properties.js",
					  "mode":"text/x-properties"},
		"python":{"file":"python/python.js",
				  "mode":"text/x-python"},
		"python2":{"mode":"text/x-python",
				   "requires":["python"],
				   "options":{"version":2}},
		"python3":{"mode":"text/x-python",
				   "requires":["python"],
				   "options":{"version":3}},
		"r":{"file":"r/r.js",
			 "mode":"text/x-rsrc"},
		"rpm-spec":{"file":"rpm/spec/spec.js",
					"mode":"text/x-rpm-spec"},
		"rpm-changes":{"file":"rpm/changes/changes.js",
					   "mode":"text/x-rpm-changes"},
		"rst":{"file":"rst/rst.js",
			   "mode":"text/x-rst"},
		"ruby":{"file":"ruby/ruby.js",
				"mode":"text/x-ruby"},
		"rust":{"file":"rust/rust.js",
				"mode":"text/x-rustsrc"},
		"scala":{"mode":"text/x-scala",
				 "requires":["clike"]},
		"scheme":{"file":"scheme/scheme.js",
				  "mode":"text/x-scheme"},
		"shell":{"file":"shell/shell.js",
				 "mode":"text/x-sh"},
		"smalltalk":{"file":"smalltalk/smalltalk.js",
					 "mode":"text/x-stsrc"},
		"smarty":{"file":"smarty/smarty.js",
				  "mode":"text/x-smarty"},
		"sparql":{"file":"sparql/sparql.js",
				  "mode":"application/x-sparql-query"},
		"stex":{"file":"stex/stex.js",
				"mode":"text/x-stex"},
		"tiddlywiki":{"file":"tiddlywiki/tiddlywiki.js",
					  "mode":"text/x-tiddlywiki"},
		"tikiwiki":{"file":"tiki/tiki.js",
					"mode":"tiki"},
		"vbscript":{"file":"vbscript/vbscript.js",
					"mode":"text/vbscript"},
		"velocity":{"file":"velocity/velocity.js",
					"mode":"text/velocity"},
		"verilog":{"file":"verilog/verilog.js",
				   "mode":"text/x-verilog"},
		"xml":{"file":"xml/xml.js",
			   "mode":"application/xml"},
		"xquery":{"file":"xquery/xquery.js",
				  "mode":"application/xquery"},
		"yaml":{"file":"yaml/yaml.js",
				"mode":"text/x-yaml"}
	}
}